<p id="notice"><%= notice %></p>

<h1>Team Challenge Memberships</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Challenge</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_challenge_memberships.each do |team_challenge_membership| %>
      <tr>
        <td><%= team_challenge_membership.team_id %></td>
        <td><%= team_challenge_membership.challenge_id %></td>
        <td><%= link_to 'Show', team_challenge_membership %></td>
        <td><%= link_to 'Edit', edit_team_challenge_membership_path(team_challenge_membership) %></td>
        <td><%= link_to 'Destroy', team_challenge_membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team Challenge Membership', new_team_challenge_membership_path %>

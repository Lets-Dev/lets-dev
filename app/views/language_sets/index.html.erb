<p id="notice"><%= notice %></p>

<h1>Language Sets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @language_sets.each do |language_set| %>
      <tr>
        <td><%= language_set.name %></td>
        <td><%= link_to 'Show', language_set %></td>
        <td><%= link_to 'Edit', edit_language_set_path(language_set) %></td>
        <td><%= link_to 'Destroy', language_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Language Set', new_language_set_path %>

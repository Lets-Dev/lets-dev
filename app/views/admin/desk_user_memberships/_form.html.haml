= form_for([:admin, desk_user_membership]) do |f|
  - if desk_user_membership.errors.any?
    #error_explanation
      %h2
        = pluralize(desk_user_membership.errors.count, "error")
        prohibited this desk_user_membership from being saved:
      %ul
        - desk_user_membership.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :desk_id_id
    = f.text_field :desk_id_id
  .field
    = f.label :user_id_id
    = f.text_field :user_id_id
  .actions
    = f.submit

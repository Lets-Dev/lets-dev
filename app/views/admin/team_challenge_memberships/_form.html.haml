= form_for([:admin, team_challenge_membership]) do |f|
  - if team_challenge_membership.errors.any?
    #error_explanation
      %h2
        = pluralize(team_challenge_membership.errors.count, "error")
        prohibited this team_challenge_membership from being saved:
      %ul
        - team_challenge_membership.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :team_id_id
    = f.text_field :team_id_id
  .field
    = f.label :challenge_id_id
    = f.text_field :challenge_id_id
  .actions
    = f.submit

%p#notice= notice
%h1 Team Challenge Memberships
%table
  %thead
    %tr
      %th Team
      %th Challenge
      %th{:colspan => "3"}
  %tbody
    - @team_challenge_memberships.each do |team_challenge_membership|
      %tr
        %td= team_challenge_membership.team_id
        %td= team_challenge_membership.challenge_id
        %td= link_to 'Show', team_challenge_membership
        %td= link_to 'Edit', edit_admin_team_challenge_membership_path(team_challenge_membership)
        %td= link_to 'Destroy', team_challenge_membership, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Team Challenge Membership', new_admin_team_challenge_membership_path

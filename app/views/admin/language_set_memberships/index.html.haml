%p#notice= notice
%h1 Language Set Memberships
%table
  %thead
    %tr
      %th Language
      %th Language set
      %th{:colspan => "3"}
  %tbody
    - @language_set_memberships.each do |language_set_membership|
      %tr
        %td= language_set_membership.language_id
        %td= language_set_membership.language_set_id
        %td= link_to 'Show', language_set_membership
        %td= link_to 'Edit', edit_admin_language_set_membership_path(language_set_membership)
        %td= link_to 'Destroy', language_set_membership, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Language Set Membership', new_admin_language_set_membership_path
